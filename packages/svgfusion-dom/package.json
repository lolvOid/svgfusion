{
  "name": "svgfusion-dom",
  "version": "1.1.0",
  "description": "Browser-optimized package for converting SVG files into React and Vue 3 components with TypeScript support and native SVG props inheritance.",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "author": "lolvoid",
  "homepage": "https://svgfusion.netlify.app",
  "keywords": [
    "svg",
    "react",
    "vue",
    "vue3",
    "component",
    "converter",
    "browser",
    "jsx",
    "tsx",
    "typescript",
    "frontend",
    "icon"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "release": "semantic-release",
    "release:dry": "semantic-release --dry-run"
  },
  "dependencies": {
    "svgfusion-core": "workspace:^",
    "svgfusion-react": "workspace:^",
    "svgfusion-utils": "workspace:^",
    "svgfusion-vue": "workspace:^"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@jest/globals": "^30.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "eslint": "^9.31.0",
    "jest": "^30.0.4",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.5",
    "semantic-release-monorepo": "^8.0.2",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.37.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
