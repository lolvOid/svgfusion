{
  "name": "svgfusion-cli",
  "version": "1.0.0",
  "description": "Command-line interface for SVGFusion",
  "license": "MIT",
  "author": "lolvoid",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "bin": {
    "svgfusion-cli": "dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "figlet": "^1.7.0",
    "svgfusion-core": "workspace:*",
    "svgfusion-utils": "workspace:*",
    "svgfusion-react": "workspace:*",
    "svgfusion-vue": "workspace:*"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@jest/globals": "^30.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "eslint": "^9.31.0",
    "jest": "^30.0.4",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.37.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
