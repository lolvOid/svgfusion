{
  "name": "svgfusion-vue",
  "version": "1.0.0",
  "description": "Vue component generator for SVGFusion",
  "license": "MIT",
  "author": "lolvoid",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "release": "semantic-release"
  },
  "dependencies": {
    "svgfusion-core": "latest",
    "svgfusion-utils": "latest"
  },
  "devDependencies": {
    "@eslint/js": "9.31.0",
    "@jest/globals": "^30.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "eslint": "^9.31.0",
    "jest": "^30.0.4",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "8.37.0"
  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
