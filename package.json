{
  "name": "svgfusion",
  "version": "1.0.5",
  "description": "A powerful CLI tool and library that converts SVG files into production-ready React and Vue 3 components with TypeScript support and automatic optimization.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "directory": "./dist",
    "access": "public"
  },
  "bin": {
    "svgfusion": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./browser": {
      "import": "./dist/browser.mjs",
      "types": "./dist/browser.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CONTRIBUTING.md",
    "LICENSE"
  ],
  "keywords": [
    "svg",
    "react",
    "vue",
    "vue3",
    "component",
    "converter",
    "cli",
    "jsx",
    "tsx",
    "typescript",
    "svgr",
    "batch",
    "optimization",
    "icon",
    "frontend",
    "build-tool"
  ],
  "author": "SVGFusion Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lolvOid/svgfusion.git"
  },
  "homepage": "https://svgfusion.netlify.app",
  "bugs": {
    "url": "https://github.com/lolvOid/svgfusion/issues"
  },
  "scripts": {
    "build": "tsup && chmod +x dist/cli.js",
    "dev": "tsup --watch",
    "test": "jest",
    "test:coverage": "jest --coverage --json --testLocationInResults --outputFile=./coverage/coverage-summary.json",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts tests/**/*.ts *.md",
    "format:check": "prettier --check src/**/*.ts tests/**/*.ts *.md",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepare": "husky install",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run test",
    "release": "semantic-release",
    "release:dry": "semantic-release --dry-run",
    "dev:docs": "cd docs && pnpm run start",
    "build:docs": "cd docs && pnpm run build",
    "postinstall": "if [ -d docs ]; then cd docs && pnpm install; fi"
  },
  "dependencies": {
    "@svgr/core": "^8.0.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/figlet": "^1.7.0",
    "chalk": "^5.0.0",
    "commander": "^11.0.0",
    "figlet": "^1.8.1",
    "ora": "^7.0.0",
    "svgo": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "husky": "^8.0.3",
    "jest": "^30.0.4",
    "lint-staged": "^15.2.2",
    "prettier": "^2.8.8",
    "semantic-release": "^24.2.6",
    "ts-jest": "^29.4.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
